                                    <th>file Date</th>
                                    <th>Remarks</th> --}}
                                </thead>
                                <tbody>
                                    {{!-- {{#each docus}}
                                    <tr>{{}}</tr>
                                    {{/each}} --}}
                                </tbody>
                            </div>
                        </div>
                    </div>
                    <div class="officerListCase">
                        <span id="officerList">List of officers</span>
                        <div class="officerList"></div>
                        <button class="manageButt" type="button">Manage Officers</button>
                    </div>
                </div>
                <div class="container" id="postacts" style="display: none;">
                    <div class="submissionsCase">
                        <div class="squares">
                            <div class="squareThings">
                                Total number
                            </div>
                            <div class="squareThings">
                                View document information
                            </div>
                            <div class="squareThings">
                                Recent number
                            </div>
                            <div class="squareThings" onclick="">
                                Encode
                            </div>
                        </div>
                        <div class="submissions">
                            <div class="dividerTwo"></div>
                            <span> Required Post-Activity Submissions</span>
                            <div class="table">
                                
                            </div>
                        </div>
                    </div>
                    <div class="officerListCase">
                        <span id="officerList">List of officers</span>
                        <div class="officerList"></div>
                        <button class="manageButt" type="button">Manage Officers</button>
                    </div>
                </div>
                <div class="container" id="others" style="display: none;">
                    <div class="submissionsCase">
                        <div class="squares">
                            <div class="squareThings">
                                Total number
                            </div>
                            <div class="squareThings">
                                View document information
                            </div>
                            <div class="squareThings">
                                Recent number
                            </div>
                            <div class="squareThings">
                                Encode
                            </div>
                        </div>
                        <div class="submissions">
                            <div class="dividerTwo"></div>
                            <span> Other Submissions</span>
                            <div class="table">
                                
                            </div>
                        </div>
                    </div>
                    <div class="officerListCase">
                        <span id="officerList">List of officers</span>
                        <div class="officerList"></div>
                        <button class="manageButt" type="button">Manage Officers</button>
                    </div>
                </div>
            </div>
        </div>
</body>

